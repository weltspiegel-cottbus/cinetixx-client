{
	"name": "lean-stack/cinetixx-client",
	"description": "Cinetixx API Client Library",
	"license": "MIT",
	"authors": [
		{
			"name": "Micha Alt",
			"email": "mail@lean-stack.de"
		}
	],
	"scripts": {
		"test": "phpunit --testdox",
		"test:coverage": "phpunit --testdox --coverage-html .coverage"
	},
	"autoload": {
		"psr-4": {
			"LeanStack\\CinetixxAPI\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"LeanStack\\CinetixxAPI\\Tests\\": "tests"
		}
	},
	"require": {
		"php": "^8.0.9",
		"symfony/cache": "^5.3",
		"symfony/http-client": "^5.3",
		"symfony/dom-crawler": "^5.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.3-dev"
		}
	}
}
