{
	"name": "lean-stack/cinetixx-client",
	"description": "Cinetixx API Client Library",
	"license": "MIT",
	"version": "2.0.0",
	"authors": [
		{
			"name": "Micha Alt",
			"email": "mail@lean-stack.de"
		}
	],
	"scripts": {
		"test": "phpunit --testdox",
		"test:coverage": "phpunit --testdox --coverage-html .coverage"
	},
	"autoload": {
		"psr-4": {
			"LeanStack\\CinetixxAPI\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"LeanStack\\CinetixxAPI\\Tests\\": "tests"
		}
	},
	"require": {
		"php": "^7.3.11",
		"symfony/http-client": "^5.0",
		"symfony/dom-crawler": "^5.0",
		"symfony/cache": "^5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.4"
	}
}
